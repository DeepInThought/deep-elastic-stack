{
  "scripts": {
    "commit": "git-cz",
    "prerelease": "git checkout master && git pull origin master",
    "version": "conventional-changelog --present eslint --infile CHANGELOG.md --same-file -release 0 && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && rm -rf build/temp"
  },
  "devDependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "name": "deep-elastic-stack",
  "description": "DeepIT Commit Workflow",
  "version": "0.1.0",
  "main": "",
